<com.cloudbees.jenkins.plugins.sshcredentials.impl.BasicSSHUserPrivateKey plugin="ssh-credentials@1.5.1">
  <id><%= @new_resource.uuid %></id>
  <description><%= @new_resource.description %></description>
  <username><%= @new_resource.username %></username>
  <passphrase><%= @new_resource.passphrase %></passphrase>
  <privateKeySource class="com.cloudbees.jenkins.plugins.sshcredentials.impl.BasicSSHUserPrivateKey$DirectEntryPrivateKeySource">
    <privateKey><%= @new_resource.key %></privateKey>
  </privateKeySource>
</com.cloudbees.jenkins.plugins.sshcredentials.impl.BasicSSHUserPrivateKey>
